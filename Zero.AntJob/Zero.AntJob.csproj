<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyTitle>大数据计算</AssemblyTitle>
    <Description>蚂蚁调度系统子程序，编写数据处理等业务逻辑，连接蚂蚁调度中心，拉取作业任务来执行</Description>
    <Company>新生命开发团队</Company>
    <Copyright>©2002-2020 NewLife</Copyright>
    <Version>1.0.2020.0803</Version>
    <FileVersion>1.0.2020.0803</FileVersion>
    <AssemblyVersion>1.0.*</AssemblyVersion>
    <Deterministic>false</Deterministic>
    <OutputPath>..\Bin\AntJob</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DefineConstants>$(DefineConstants);DEBUG</DefineConstants>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NewLife.Agent" Version="8.8.2021.1204" />
    <PackageReference Include="NewLife.AntJob" Version="2.4.2021.1204" />
    <PackageReference Include="NewLife.AntJob.Extensions" Version="2.4.2021.1204" />
    <PackageReference Include="NewLife.Core" Version="8.11.2021.1204" />
    <PackageReference Include="NewLife.Redis" Version="3.9.2021.1204" />
    <PackageReference Include="NewLife.Stardust" Version="1.5.2021.1204" />
    <PackageReference Include="NewLife.XCode" Version="10.3.2021.1204" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Zero.Core\Zero.Core.csproj" />
    <ProjectReference Include="..\Zero.Data\Zero.Data.csproj" />
  </ItemGroup>

</Project>
